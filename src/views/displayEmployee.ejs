<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Get CRUD</title>
    <style>
      table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
      }
      .btn-edit,
      .btn-delete {
        border: none;
        background-color: orange;
        padding: 5px;
        border-radius: 10px;
        cursor: pointer;
        text-decoration: none;
      }
      .btn-delete {
        background-color: red;
      }

      td,
      th {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
        text-align: center;
      }

      tr:nth-child(even) {
        background-color: #dddddd;
      }
    </style>
  </head>
  <body>
    <table>
      <tr>
        <th>EmployeeId</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>vacationDays</th>
        <th>paidtoDate</th>
        <th>paidLastYear</th>
        <th>paidRate</th>
        <th>paidRateId</th>
      </tr>
      <% for (let i = 0; i < dataTable.length; i++) { %>
      <tr>
        <td><%= dataTable[i].employeeId %></td>
        <td><%= dataTable[i].firstName %></td>
        <td><%= dataTable[i].lastName %></td>
        <td><%= dataTable[i].vacationDays %></td>
        <td><%= dataTable[i].paidToDate %></td>
        <td><%= dataTable[i].paidLastYear %></td>
        <td><%= dataTable[i].payRate %></td>
        <td><%= dataTable[i].payRateId %></td>
      </tr>
      <% } %>
    </table>
  </body>
</html>
